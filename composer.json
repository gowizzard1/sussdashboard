{
    "name": "phalcon/siwebapp",
    "type": "library",
    "description": "This is the dashboard for suss ads",
    "keywords": [
        "siwebapp",
        "framework",
        "starter app",
        "Sobbayi"
    ],
    "homepage": "https://sobbayi.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Steve Obbayi",
            "email": "steven.richard@sobbayi.com"
        } 
    ],
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*",
        "ext-phalcon": "~4.1.0",
        "robmorgan/phinx": "^0.11.1",
        "swiftmailer/swiftmailer": "^6.2",
        "vlucas/phpdotenv": "^3.6",
        "rmccue/requests": "^1.8"
    },
    "require-dev": {
        "codeception/codeception": "^3.1",
        "phpunit/phpunit": "^8.4",
        "squizlabs/php_codesniffer": "3.5",
        "vimeo/psalm": "^3.7"
    },
    "suggest": {
        "ext-apc": "Needed to support caching ACL"
    },
    "autoload": {
        "psr-4": {
            "Siwebapp\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "archive": {
        "exclude": [
            "/.ci",
            ".dockerignore",
            ".travis.yml",
            "CODE_OF_CONDUCT.md",
            "CONTRIBUTING.md"
        ]
    }
}
